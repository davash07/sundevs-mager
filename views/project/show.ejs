<% include ../base/_header.ejs %>
<% include ../base/_navbar.ejs %>
<% include ../base/_aside.ejs %>
<div class="col-sm-9">
    <div class="row">
        <div class="container">
            <div class="row" style="margin-top: 5%" >
                <div class="col-sm-9">
                <h2 class="text-center">Project Details</h2>
                </div>
                <div class="col-sm-6">
                    <h4>Name : <%= project.name %></h4>
                </div>
                <div class="col-sm-6">
                    <h4>Status : <%= project.status %></h4>
                </div>
            </div>
            <div class="row">

                <div class="col-sm-6">
                    <h4 style="margin-top: 4.5%">Estimated End Date : <%= project.estimated_end_date%></h4>
                    <h4 style="margin-top: 4.5%">Real End Date : <%= project.real_end_date%></h4>
                    <h4 style="margin-top: 4.5%">Challenging Date : <%= project.challenging_date%></h4>
                    <h4 style="margin-top: 4.5%">Challenging Bugs : <%= project.challenging_bugs %></h4>
                </div>
                <div class="col-sm-6">
                    <h4>Activities</h4>
                    <% for(var i = 0; i < project.activities.length; i++){%>
                        <h4>* <%= project.activities[i] %></h4>
                    <%}%>
                </div>
            </div>
            <div class="row">
                <div class="col-sm-9 text-center">
                    <a class="btn btn-info" href="/projects">Back</a>
                </div>
            </div>
        </div>

<% include ../base/_footer.ejs %>